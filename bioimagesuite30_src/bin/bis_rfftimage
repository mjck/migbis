    source ${BIOIMAGESUITE3_BINARY_DIR}/setpaths.sh > /dev/null
else
    source ${BASE}/setpaths.sh > /dev/null
fi

#echo "Running ${BIS_VTK_DIR}/../../bin/vtk ${BASE}/bis_algorithm/bis_rfftimage.tcl"

${BIS_VTK_DIR}/../../bin/vtk ${BASE}/bis_algorithm/bis_rfftimage.tcl -- $@


